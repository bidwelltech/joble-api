{
  "name": "joble-api",
  "version": "0.0.0",
  "main": "server/server.js",
  "scripts": {
    "lint": "eslint .",
    "start": "npm run start:development",
    "start:development": "NODE_ENV=development nodemon .",
    "start:production": "NODE_ENV=production nodemon .",
    "test": "find . -path '*models/*test.js' -not -path '*node_modules*' | NODE_ENV=test xargs mocha --bail",
    "migrate": "NODE_ENV=production node ./server/bin/automigrate.js && NODE_ENV=development node ./server/bin/automigrate.js && NODE_ENV=test node ./server/bin/automigrate.js",
    "posttest": "npm run lint && nsp check"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "debug": "^2.2.0",
    "helmet": "^1.3.0",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^2.4.0",
    "loopback-connector-postgresql": "^2.7.0",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^1.0.1",
    "supertest": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "nsp": "^2.1.0"
